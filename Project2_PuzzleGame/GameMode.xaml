<Window x:Class="Project2_PuzzleGame.GameMode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project2_PuzzleGame"
        mc:Ignorable="d"
        Title="GameMode" Height="200" Width="300">
    <Canvas>
        <Label Content="Time"></Label>
        <RadioButton Name="Unlimited" GroupName="TimeMode" Content="Unlimited" Canvas.Left="41" Canvas.Top="6"></RadioButton>
        <RadioButton Name="Three_Minutes" GroupName="TimeMode" Content="3 Minutes" Canvas.Left="41" Canvas.Top="26"></RadioButton>
        <Button Name="OK" Click="OK_Click" Content="OK" Width="50" Height="25" Canvas.Left="125" Canvas.Top="100"/>
    </Canvas>
</Window>
